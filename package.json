{
  "name": "@ngrx/db",
  "version": "1.0.1",
  "description": "RxJS + IndexedDB for Angular2",
  "main": "index.js",
  "scripts": {
    "test": "karma start --ci",
    "prepare": "npm run clean && npm run build && npm run copy_package",
    "copy_package": "cp -r package.json dist/package.json",
    "watch": "karma start",
    "build": "tsc",
    "build_test": "tsc -p ./spec",
    "clean": "rm -rf dist",
    "clean_example": "rm -rf example_dist",
    "example": "npm run clean_example && tsc -p ./example && http-server"
  },
  "author": "Rob Wormald <robwormald@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.1",
    "rxjs": "^5.0.0-beta.6"
  },
  "devDependencies": {
    "@angular/core": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "http-server": "^0.8.5",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "reflect-metadata": "^0.1.2",
    "systemjs": "^0.19.11",
    "typescript": "^1.7.5",
    "yargs": "^3.31.0",
    "zone.js": "^0.5.10"
  },
  "typings": "dist/database.d.ts"
}
